{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["README.md", "src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "destination": "docs/",
        "readme": "README.md",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "title": "DisTube",
            "filter": false,
            "menu": [{
                    "title": "NPM",
                    "link": "https://www.npmjs.com/package/distube",
                    "target": "_blank"
                },
                {
                    "title": "Github",
                    "link": "https://github.com/skick1234/DisTube/",
                    "target": "_blank"
                }
            ],
            "meta": [
                "<meta name=\"author\" content=\"Skick\">",
                "<meta name=\"description\" content=\"A Discord.js v12 module to easily manage music commands and play Youtube song on Discord without any API key.\">"
            ],
            "search": true,
            "footer": "Powered by <a href='https://jsdoc.app/' target='_blank'>JSDoc</a>, <a href='https://github.com/ankitskvmdam/clean-jsdoc-theme' target='_blank'>clean-jsdoc-theme</a>, <a href='https://js.org' target='_blank'>js.org</a> and Love"
        }
    }
}